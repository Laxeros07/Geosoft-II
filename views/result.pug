extends layout

block content
  .row.gy-4.gx-lg-4.justify-content-md-center.mt-4 
    .col-lg-8.col-xs-12.mb-3
      .card.text-dark.shadow.p-4.h-100
        #map
    .col-lg-4.col-xs-12.mb-3
      .card.text-dark.shadow.p-4
        div
          p#bereich1 
          p#bereich2
            .card.text-dark.shadow.p-4(style='height: 300px;overflow: scroll;')
              table#tabelle.table.table-dark.table-striped.table-hover
                tr
                  th Label
                  th Class_ID


        div 
          form(method='post' action='result')
            button.btn.btn-primary.my-2(type='submit') Hochladen
            button.btn.btn-2.my-2.m-2(onclick='geojsonExport()') Export Data


    script.
      // Set active Button
      document.querySelectorAll(".active")[0].classList.remove("active")
      document.querySelectorAll(".nav-link")[2].classList.add("active")




    script(src='/javascripts/result.js')


